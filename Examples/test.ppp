
!% --------------------------------------------------------------- %!

варибля РешитьКвадратку ( варибля а , варибля б , варибля ц )
!:  
    проверочка ( а == 0 )
    !:        
        ретерн РешитьЛинейное ( б , ц )
    :!

    варибля Д = б * б - 4 * а * ц ;

    проверочка ( Д > 0 )
    !:
        принтф ( ( ( 0 - б ) + кор ( Д ) ) / ( 2 * а ) , ( ( 0 - б ) - кор ( Д ) ) / ( 2 * а ) )

        ретерн 2
    :!

    проверочка ( Д == 0 )
    !:
        принтф ( ( 0 - б ) / ( 2 * а ) )

        ретерн 1
    :!

    ретерн 0
:!

!% --------------------------------------------------------------- %!

варибля РешитьЛинейное ( варибля а , варибля б )
!:
    проверочка ( а != 0 ) 
    !:  
        принтф ( ( 0 - б ) / а ) 

        ретерн 1
    :!

    проверочка ( б == 0 )
    !:
        принтф ( 99999 ) 

        ретерн 99999 
    :!

    ретерн 0 ;
:!

!% --------------------------------------------------------------- %!

варибля Факториал ( варибля н )
!:
    проверочка н <= 1
    !:
        ретерн 1
    :!
    
    ретерн ( Факториал ( н - 1 ) ) * н
:!

!% --------------------------------------------------------------- %!

варибля Мейн ( )
!:
    варибля ф = 5

    !% сканф ( ф ) %!
    
    принтф ( Факториал ( ф ) )
    
    варибля а = 1 
    варибля б = -2
    варибля ц = 1

    !% сканф ( а , б , ц , р ) %!

    варибля количество_корней = РешитьКвадратку ( а , б , ц )

    ретерн 0
:!

