!% --------------------------------------------------------------- %!

!%
варибля инфинити = 9999

варибля инфинити2 = -2
%!

!% --------------------------------------------------------------- %!

варибля РешитьКвадратку ( варибля а , варибля б , варибля ц )
!:  
    проверочка ( а == 0 )
    !:        
        ретерн РешитьЛинейное ( б , ц )
    :!

    варибля Д = б ^ 2 - 4 * а * ц ;

    проверочка ( Д > 0 )
    !:
        принтф ( ( ( 0 - б ) + кор ( Д ) ) / ( 2 * а ) , ( ( 0 - б ) - кор ( Д ) ) / ( 2 * а ) )

        ретерн 2
    :!

    проверочка ( Д == 0 )
    !:
        принтф ( ( 0 - б ) / ( 2 * а ) )

        ретерн 1
    :!

    ретерн 0
:!

!% --------------------------------------------------------------- %!

варибля РешитьЛинейное ( варибля а , варибля б )
!:
    проверочка ( а != 0 ) 
    !:  
        принтф ( ( 0 - б ) / а ) 

        ретерн 1
    :!

    проверочка ( б == 0 )
    !:
        принтф ( 99999 ) 

        ретерн 99999 
    :!

    ретерн 0 ;
:!

!% --------------------------------------------------------------- %!

варибля Факториал ( варибля н )
!:
    проверочка н <= 1
    !:
        ретерн 1
    :!
    
    ретерн ( Факториал ( н - 1 ) ) * н
:!

!% --------------------------------------------------------------- %!

варибля Мейн ( )
!:
    варибля ф = 0

    сканф ( ф )
    
    принтф ( Факториал ( ф ) )
    
    варибля а = 0 
    варибля б = 0
    варибля ц = 0

    сканф ( а , б , ц )

    РешитьКвадратку ( а , б , ц )

    !% принтф ( а ) %!

    ретерн 0
:!

!% --------------------------------------------------------------- %!